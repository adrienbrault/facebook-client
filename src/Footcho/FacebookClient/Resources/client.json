{
    "operations": {
        "getObject": {
            "httpMethod": "GET",
            "uri": "/{id}",
            "class": "Guzzle\\Service\\Command\\OperationCommand",
            "parameters": {
                "id": {
                    "type": ["string", "integer"],
                    "required": true,
                    "location": "uri"
                },
                "access_token": {
                    "type": "string",
                    "required": false,
                    "location": "header",
                    "sentAs": "Authorization",
                    "filters": [
                        "Footcho\\FacebookClient\\Helper\\AuthorizationHelper::getAuthorizationFromAccessToken"
                    ]
                }
            }
        }
    }
}
